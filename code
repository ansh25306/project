def sum(a,b):
    sum=a+b
    print('sum=',sum)

def subtraction(a,b):
    subtraction=a-b
    print('subtraction=',subtraction)

def product(a,b):
    product=a*b
    print('product=',product)

def division(a,b):
    division=a/b
    print('division=',division)

def exponential(a,b):
    exponential=a**b
    print('exponential=',exponential)
